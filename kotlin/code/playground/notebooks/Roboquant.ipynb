    "val feed = AvroFeed.sp500()\n",
    "\n",
    "val emaStrategy = EMAStrategy()\n",
    "val rsiStrategy = RSIStrategy()\n",
    "val strategy = ParallelStrategy(emaStrategy, rsiStrategy)\n",
    "\n",
    "val roboquant = Roboquant(strategy, AccountMetric())\n",
    "roboquant.run(feed)"
